@model IEnumerable<VIP_Parking.Models.Database.Requester>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>
<div style="margin-bottom:25px;">
    @Html.ActionLink("Back to Reservations", "Index", "Reservations", null, new { @class = "link_button purple" })
</div>
<ul id="users_list">
    @foreach (var item in Model)
    {
        <li>
         @Html.ActionLink(item.Firstname+" "+item.Lastname, "Edit", new { id = item.Requester_ID })
        </li>
    }
</ul>

